   "source": [
    "#溫度轉換器\n",
    "unit=input(\"請輸入當前溫度單位\")\n",
    "temp=float(input(\"請輸入當前溫度\"))\n",
    "\n",
    "#判斷兩個值是否相等時必須使用 ==\n",
    "if unit==\"c\":\n",
    "    temp=round(temp*(9/5)+32)\n",
    "    new_nuit=\"度f\"\n",
    "elif unit==\"f\":\n",
    "    temp=(temp-32)*5/9\n",
    "    new_nuit=\"度c\"\n",
    "else:\n",
    "    print(\"單位錯誤\")\n",
    "    \n",
    "print(f\"當前溫度為{temp}{new_nuit}\")\n"
   ]
  }
 ],
